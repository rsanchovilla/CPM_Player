10 A$=" "
'C IS THE LOCATION WHERE A$ STORAGE BEGINS
        B=VARPTR(A$):C=IPEEK(B)
        PRINT"50 ITERATIONS"
        FOR M=1 TO 50
        COUNT=0
        A$=STRING$(8191,1)
20 D=C-1
80 FOR I=0 TO 8190
90 ++D:B=PEEK(D):IF B=0 THEN GOTO 170
100 PRIME=I+I+3
'                  PRINT PRIME
        K=PRIME+I
120 IF K>8190 THEN GOTO 160
130 E=C+K:POKE E,0
        K=K+PRIME
        GOTO 120
160 ++COUNT
170 NEXT
180 NEXT
190 PRINT COUNT;:PRINT" PRIMES."
200 END
